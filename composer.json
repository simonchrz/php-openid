{
    "name": "openid/php-openid",
    "version": "2.3.0",
    "description": "OpenID library for PHP5",
    "keywords": ["openid", "authentication", "yadis", "auth"],
    "license": "Apache-2.0",
    "homepage": "http://github.com/openid/php-openid",
    "authors": [
        {
            "name": "JanRain Inc.",
            "homepage": "http://www.openidenabled.com"
        }
    ],
    "repositories": [
        {
            "type": "pear",
            "url": "https://pear.php.net"
        }
    ],
    "require": {
        "php": ">=4.3",
        "ext-bcmath": "*",
        "ext-curl": "*",
        "ext-dom": "*",
        "pear-pear.php.net/MDB2": "*"
    },
    "suggest": {
        "ext-gmp": "*"
    },
    "require-dev": {
        "pear-pear.php.net/PHPUnit": "^1.0"
    },
    "autoload": {
        "classmap": ["Auth"]
    },
    "autoload-dev": {
        "classmap": ["Tests/Auth"]
    },
    "include-path": ["."]
}
